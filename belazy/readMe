curl commands
curl --http2 -I -k GET "http://localhost:9090/v1/grpc/hello"
curl --http2 http://localhost:9090/v1/grpc/hello

curl --http2 http://localhost:9090/v1/grpc/hello
curl -k -v --http2 http://localhost:9090/v1/grpc/hello
curl -k -v --http2 https://localhost:9090/v1/grpc/hello

curl -k -v --http2-prior-knowledge http://localhost:9090/v1/grpc/hello